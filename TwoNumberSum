import java.util.*;

class Program {
  public static int[] twoNumberSum(int[] array, int targetSum) {
		int[] nums = new int[2];
		int[] empty = new int[0];
		int q = 0;
    for(int i = 0; i < array.length; i++){
			for(int j = 0 + i; j < array.length; j++){
				if(array[i] + array[j] == targetSum && array[i] != array[j]){
				nums[0] = array[i];
				nums[1] = array[j];
					break;
			}
		}
  }
		if(nums[0] + nums[1] == targetSum)
		return nums;
		else
		return empty;
}
}
